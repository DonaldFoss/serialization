{
  "name": "capnp-js-base",
  "version": "0.2.2",
  "description": "Capnproto run-time builder.",
  "devDependencies": {
    "buffers": "^0.1.1",
    "capnp-js-plugin": "^0.2.2",
    "gulp": "^3.8.7",
    "gulp-jshint": "^1.8.4",
    "gulp-nfy": "0.0.1",
    "gulp-preprocess": "^1.1.1",
    "gulp-uglify": "^0.3.1",
    "int64-native": "^0.3.2",
    "mocha": "^1.21.4",
    "nfy": "0.0.1",
    "node-inspector": "^0.8.1",
    "pegjs": "^0.8.0",
    "ramda": "^0.8.0",
    "when": "^3.6.3"
  },
  "scripts": {
    "build": "gulp build",
    "schema": "mkdir -p tmp && capnp compile -ojs:tmp --src-prefix=test test/testing.capnp",
    "testing-capnp": "npm run schema && nfy --output=test/testing.capnp.d --src-prefix=tmp/testing.capnp.d/ tmp/testing.capnp.d/*.js",
    "test": "npm run build && npm run testing-capnp && mocha test/structure test/list",
    "test-debug": "mocha --debug-brk test/structure test/list & node-inspector & wait"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/popham/capnp-js-base"
  },
  "author": "Tim Popham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/popham/capnp-js-base/issues"
  },
  "homepage": "https://github.com/popham/capnp-js-base",
  "private": true
}
